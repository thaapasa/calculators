{
  "name": "calculators",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Handy calculator tools in an easily-accessible web page",
  "homepage": "http://laskurit.pomeranssi.fi",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest",
    "test-ci": "cross-env CI=TRUE jest",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/",
    "eject": "react-scripts-ts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaapasa/calculators"
  },
  "dependencies": {
    "baconjs": "2.0.7",
    "cross-env": "5.2.0",
    "crypto-browserify": "3.12.0",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "material-ui": "0.20.1",
    "moment": "2.22.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/baconjs": "0.7.33",
    "@types/jest": "23.1.5",
    "@types/material-ui": "0.21.3",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/uuid": "^3.4.3",
    "jest": "23.4.0",
    "react-scripts-ts": "2.16.0",
    "ts-jest": "23.0.0",
    "tslib": "1.9.3",
    "tslint": "5.10.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
