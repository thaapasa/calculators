{
  "name": "calculators",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Handy calculator tools in an easily-accessible web page",
  "homepage": "http://laskurit.pomeranssi.fi",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest --runInBand",
    "test-ci": "cross-env CI=TRUE jest --runInBand",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaapasa/calculators"
  },
  "dependencies": {
    "baconjs": "2.0.10",
    "cross-env": "5.2.0",
    "crypto-browserify": "3.12.0",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "material-ui": "0.20.2",
    "moment": "2.23.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/baconjs": "0.7.33",
    "@types/jest": "23.3.12",
    "@types/material-ui": "0.21.5",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/uuid": "3.4.4",
    "jest": "23.6.0",
    "react-scripts-ts": "3.1.0",
    "ts-jest": "23.10.5",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "typescript": "3.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
