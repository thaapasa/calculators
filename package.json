{
  "name": "calculators",
  "version": "1.2.1",
  "license": "MIT",
  "description": "Handy calculator tools in an easily-accessible web page",
  "homepage": "https://laskurit.pomeranssi.fi",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest --runInBand",
    "test-ci": "cross-env CI=TRUE jest --runInBand",
    "deploy": "sh script/deploy.sh",
    "clean": "rm -rf build/",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaapasa/calculators"
  },
  "dependencies": {
    "@material-ui/core": "4.5.0",
    "@material-ui/icons": "4.4.3",
    "baconjs": "3.0.10",
    "cross-env": "6.0.3",
    "crypto-browserify": "3.12.0",
    "history": "4.10.1",
    "intl": "1.2.5",
    "intl-locales-supported": "1.6.0",
    "luhn-js": "1.1.1",
    "moment": "2.24.0",
    "ramda": "0.26.1",
    "react": "16.10.1",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.10.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-sizeme": "2.6.7",
    "styled-components": "4.4.0",
    "svg-rn": "0.1.2",
    "uuid": "3.3.3",
    "xml-js": "1.6.11",
    "xml2js": "0.4.22"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/ramda": "0.26.26",
    "@types/react": "16.9.4",
    "@types/react-autosuggest": "9.3.11",
    "@types/react-dom": "16.9.1",
    "@types/react-router": "5.1.1",
    "@types/react-router-dom": "5.1.0",
    "@types/styled-components": "4.1.19",
    "@types/uuid": "3.4.5",
    "@types/xml2js": "0.4.5",
    "prettier": "1.18.2",
    "react-scripts-ts": "3.1.0",
    "ts-jest": "24.1.0",
    "tslib": "1.10.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
